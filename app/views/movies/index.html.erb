<div class="page-wrapper">

  <header>
  </header>
    
  <main>

    <form>
      <label>
        <input type="text" name="new_movie" placeholder="Enter movie title...">
      </label>
      <button>Add Movie</button>
    </form>

    <ul class="movies seen-movies">
      <h2>Already Seen</h2>
    </ul>

    <ul class="movies unseen-movies">
      <h2>Want to See</h2>
    </ul>
  </main>

  <footer>
  </footer>

</div>

<script type="template" class="movie_template">
  <li class="<%%= movie.get('seen') ? 'seen' : '' %>">
    <h3 class="movie-title"><%%= movie.get('title') %></h3>
    <img src="<%%= movie.get('poster') %>"><br/>
    Seen 
    <input type="checkbox" <%%= movie.get('seen')? 'checked' : '' %> />
    <span>&times;</span>
  </li>
</script>